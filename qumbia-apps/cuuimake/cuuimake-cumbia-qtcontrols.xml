 </widget>

<widget class="pool" autodetect="true"
   regexp="(%1)\(.*(CumbiaPool\s*\*).*(CuControlsFactoryPool).*\);">
   <params factory="cumbiapool">
       <!-- parameter adding occurs in the given order -->
       <param add="CumbiaPool *"  />
       <param add="CuControlsFactoryPool"  />
   </params>

</widget>

